  <style>
  #editors-edit{
     overflow: auto;
    }
    #container{
      margin:30px auto;
    }
    #grammarexercise{
      color:black;
    }
    #helpbutton{
      display:none;
    }
    #description{
      color:black;
      text-align:center;
      margin-top:12px;
     }

    #graph {
      margin-top: 16px;
      margin-bottom: 16px;
      width: 45%;
      height: 440px;

      outline: 1px black solid;
      border: 10px transparent solid;
      display: inline-block;
    }

    #av {
      margin-top: 16px;
      margin-bottom: 16px;
      width: 45%;
      height: 440px;
      display: inline-block;
    }

    .hide {
      display: none;
    }
  </style>
</head>

<body id="editors-edit">
  <!-- We now need to add some buttons related to grading the answer, and showing the results. These buttons taken from 
        FATester and the functionality of them are located on the ExerciseController JS file-->
  <div id="container">
    <img id="check_mark" src="\OpenDSA-master\AV\OpenFLAP\green_check.png" alt="Proficient" style="display: none;float: left; width: 40px;">
    <h1 id="grammarexercise">Grammar Exercise</h1>
    <button id="helpbutton"><img src="\OpenDSA-master\AV\OpenFLAP\help.png" /></button>
    <div id="help" title="Help" style="display:none">
      <div id="helpInfo">
        Help:
        To delete, click on "Delete" and click on the row you want to delete.
        To add, click on "Add Row" and click on any row to add a new row.
      </div>
    </div>
    <h2 id="question">Write a grammar that generates the language: L(<span id="expression"></span>)</h2>
    <h2 id="description"></h2>
    <div id="dialogueoverlay"></div>
    <div id="dialoguebox">
      <div>
        <div id="dialogueboxhead"></div>
        <div id="dialogueboxbody"></div>
        <div id="dialogueboxfoot"></div>
      </div>
    </div>
    <p align="center" class="arrayPlace"></p>
    <button type="button" id="editbutton" style="margin-left:60px;">Edit Grammar</button>
    <button type="button" id="addrowbutton"style="margin-left:48px;">Add Row</button>
    <button type="button" id="deletebutton"style="margin-left:48px;">Delete Row</button>
    <button type="button" class="function" id="identifybutton"style="margin-left:48px;">Identify Grammar</button>
    <div id="buttons" align="center">
      <span id='mode'></span><br>
      <div id="av">
        <a class="jsavsettings" href="#">Settings</a>
        <p align="center" class="jsavexercisecontrols"></p>
        <p class="jsavscore"></p>
        <p class="jsavoutput jsavline"></p>

      </div>
      <div id="graph" class="jsavcanvas"></div>
      <!--<div id="files">
        <input id="savefile" type="button" value="Save">
        <input id="loadfile" type="file">
        <div style='display: none;' id='download'></div>
      </div>-->
    <p id="percentage"></p>
      <table id="testResults"></table>
      </div>
    <script src="\OpenDSA-master\AV\OpenFLAP\exercises\exerController\grammarHandler.js"></script>

    <script src="\OpenDSA-master\AV\OpenFLAP\exercises\exerController\TestCaseGenerater.js"></script>
    <script src="\OpenDSA-master\lib\jquery.min.js"></script>
<script src="\OpenDSA-master/lib/jquery.migrate.min.js"></script>
    <script src="\OpenDSA-master/lib/jquery.ui.min.js"></script>
    <script src="\OpenDSA-master/lib/jquery.transit.js"></script>
    <script src="\OpenDSA-master/lib/raphael.js"></script>
    <script src="\OpenDSA-master/lib/dagre.min.js"></script>
    <script src="\OpenDSA-master/lib/JSAV.js"></script>
    <script src="\OpenDSA-master/lib/odsaUtils.js"></script>
    <script src="\OpenDSA-master/lib/odsaAV.js"></script>
    <script src="\OpenDSA-master\DataStructures\FLA\serializableGraph.js"></script>
    <script src="\OpenDSA-master/lib/underscore.js"></script>
    <script src="\OpenDSA-master/DataStructures/FLA/FA.js"></script>
    <script src="\OpenDSA-master/DataStructures/FLA/PDA.js"></script>
    <script src="\OpenDSA-master/DataStructures/FLA/Grading.js"></script>
    <script src="\OpenDSA-master\AV\OpenFLAP\grammarEditor.js"></script>
    <script src="\OpenDSA-master/DataStructures/FLA/PDA.js"></script>
    <script src="\OpenDSA-master\AV\OpenFLAP\exercises\exerController\GrammarExerciseController.js"></script>
</body>

</html>
